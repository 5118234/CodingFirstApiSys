<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="team.fjut.cf.mapper.JudgeResultMapper">

    <select id="selectByJudgeId" resultType="team.fjut.cf.pojo.po.JudgeResult">
        SELECT  judge_id,
                info
        FROM t_judge_result
        WHERE judge_id = #{judgeId}
    </select>
</mapper>