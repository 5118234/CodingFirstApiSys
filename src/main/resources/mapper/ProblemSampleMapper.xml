<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="team.fjut.cf.mapper.ProblemSampleMapper">
    <select id="selectByProblemId" resultType="team.fjut.cf.pojo.po.ProblemSamplePO">
        SELECT 	id,
        problem_id,
        case_order,
        input_case,
        output_case
        FROM
        t_problem_sample
        WHERE problem_id = #{problemId}
    </select>
</mapper>